{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "./build-deploy.sh",
  "outputDirectory": ".next",
  "installCommand": "cd ../.. && bun install",
  "devCommand": "cd ../.. && turbo dev --filter=@formfunction/marketing",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD -- apps/marketing/",
  "crons": [],
  "functions": {},
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "rewrites": [],
  "redirects": [],
  "cleanUrls": true,
  "trailingSlash": false
}
