{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/main"
    }
  ],
  "functions": {
    "api/main.py": {
      "runtime": "@vercel/python@3.11.0"
    }
  },
  "env": {
    "API_BASE_URL": "http://localhost:8080"
  }
}
